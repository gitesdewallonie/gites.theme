<?xml version="1.0"?>
<object name="portal_css">

    <stylesheet title="Gites Theme Styles"
                id="++theme++gites.theme/css/style.css"
                media="screen" 
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                applyPrefix="True"
                expression="request/HTTP_X_THEME_ENABLED | nothing"/>

    <stylesheet title="Gites Print Styles"
                id="++theme++gites.theme/css/print.css"
                media="print"
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="save"
                cookable="True"
                enabled="1"
                applyPrefix="True"
		        expression=""/>

    <stylesheet title="Chosen css"
	            id="++resource++plonewidgets/chosen.css"
                media="screen"
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                applyPrefix="True"
                expression="request/HTTP_X_THEME_ENABLED | nothing"/>


</object>
