<?xml version="1.0"?>
<object name="portal_css">

    <stylesheet id="columns.css"
                enabled="0" />

    <stylesheet id="mobile.css"
                enabled="0" />

    <stylesheet title="Bootstrap Carousel Styles"
                id="++theme++gites.theme/bootstrap-carousel/css/bootstrap.min.css"
                media="screen" 
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                applyPrefix="True"
                expression="request/HTTP_X_THEME_ENABLED | nothing"/>

    <stylesheet title="Gites Theme Styles"
                id="++theme++gites.theme/css/style.css"
                media="screen"
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                applyPrefix="True"
                expression="request/HTTP_X_THEME_ENABLED | nothing"/>

    <stylesheet title="Gites Theme Styles"
                id="++theme++gites.theme/css/ie.css"
                media="screen"
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                applyPrefix="True"
                conditionalcomment="IE"
                expression="request/HTTP_X_THEME_ENABLED | nothing"/>

    <stylesheet title="Gites Theme Styles"
                id="++theme++gites.theme/css/proprio.css"
                media="screen"
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                applyPrefix="True"
                expression="python:here.restrictedTraverse('@@isProprio')()"/>

    <stylesheet title="Gites Theme Styles"
                id="++theme++gites.theme/css/mobile.css"
                media="screen"
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                applyPrefix="True"
                expression="python:here.restrictedTraverse('@@isMobile')()"/>

    <stylesheet title="Gites Print Styles"
                id="++theme++gites.theme/css/print.css"
                media=""
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="save"
                cookable="True"
                enabled="1"
                applyPrefix="True"
                expression=""/>

    <stylesheet title="Chosen css"
                id="++resource++plonewidgets/chosen.css"
                media="screen"
                rel="stylesheet"
                rendering="import"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                applyPrefix="True"
                expression="request/HTTP_X_THEME_ENABLED | nothing"/>

    <stylesheet title=""
                id="http://fonts.googleapis.com/css?family=Josefin+Sans:700"
                media="screen"
                rel="stylesheet"
                rendering="link"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                applyPrefix="True"
                expression="request/HTTP_X_THEME_ENABLED | nothing"/>

    <stylesheet title=""
                id="http://fonts.googleapis.com/css?family=Gloria+Hallelujah"
                media="screen"
                rel="stylesheet"
                rendering="link"
                cacheable="True"
                compression="safe"
                cookable="True"
                enabled="1"
                applyPrefix="True"
                expression="request/HTTP_X_THEME_ENABLED | nothing"/>

</object>
